<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" style="-fx-background-color: #222;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.averageflow.nederlandsleraar.controllers.StartScreenController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="De of Het ?">
      <font>
         <Font name="Lato Black Italic" size="32.0" />
      </font></Text>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
      <children>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #FFF; -fx-font-weight: bold; -fx-font-family: &quot;Times New Roman&quot;; -fx-font-size: 32;" text="" />
         <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Button fx:id="buttonDe" mnemonicParsing="false" onAction="#onClickButtonDe" style="-fx-border-color: #FFF; -fx-border-radius: 12px; -fx-background-radius: 12px; -fx-background-color: #000;" text="de" textFill="WHITE">
                  <HBox.margin>
                     <Insets right="10.0" />
                  </HBox.margin>
                  <font>
                     <Font name="Lato Black Italic" size="21.0" />
                  </font>
               </Button>
               <Button fx:id="buttonHet" mnemonicParsing="false" onAction="#onClickButtonHet" style="-fx-background-color: #000; -fx-border-radius: 12px; -fx-background-radius: 12px; -fx-border-color: #FFF;" text="het" textFill="WHITE">
                  <HBox.margin>
                     <Insets left="10.0" />
                  </HBox.margin>
                  <font>
                     <Font name="Lato Black Italic" size="21.0" />
                  </font>
               </Button>
            </children>
         </HBox>
      </children>
      <padding>
         <Insets top="40.0" />
      </padding>
   </VBox>
</VBox>
